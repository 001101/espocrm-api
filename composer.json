{
  "name": "ddanninger/espocrm-api",
  "description": "PHP API for EspoCRM",
  "license": "MIT",
  "keywords": [
    "espocrm",
    "restful api"
  ],
  "homepage": "https://github.com/ddanninger/espocrm-api",
  "type": "library",
  "authors": [
    {
      "name": "Dominik Danninger",
      "email": "ddanninger@gmail.com",
      "homepage": "http://drei.kr"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ddanninger/espocrm-api"
    }
  ],
  "autoload": {
    "psr-4": {
        "EspoCRM\\Api\\": "src/",
        "EspoCRM\\Api\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": "~4|~5",
    "guzzlehttp/guzzle-services": "~0.3|^0.4"
  },
  "require-dev": {
	"phpunit/phpunit": "~4.8|^5"
  }
}
