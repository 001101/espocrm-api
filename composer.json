{
  "name": "ddanninger/espocrm-api",
  "description": "PHP API for EspoCRM",
  "license": "MIT",
  "keywords": [
    "espocrm",
    "restful api"
  ],
  "homepage": "https://github.com/ddanninger/espocrm-api",
  "type": "library",
  "authors": [
    {
      "name": "Dominik Danninger",
      "email": "ddanninger@gmail.com",
      "homepage": "http://drei.kr"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ddanninger/espocrm-api"
    }
  ],
  "config": {
    "vendor-dir": "vendor/"
  },
  "autoload": {
    "psr-4": {
       "EspoCRM\\ApiClient\\": "src"
    }
  },
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
	"phpunit/phpunit": "~4.0"
  }
}
